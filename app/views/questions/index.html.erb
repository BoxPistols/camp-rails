<h2>質問一覧</h2>

<table class="table">
    <thead>
        <tr>
            <th scope="col">#id</th>
            <th scope="col">title</th>
            <th scope="col">Menu</th>
        </tr>
    </thead>
    <tbody>
        <% @questions.each do |qs| %>
            <tr>
                <td>
                    <%= qs.id %>
                </td>
                <td>
                    <%= qs.title %>
                </td>
                <td>[edit] [delete]</td>
            </tr>
            <% end %>
    </tbody>
</table>
